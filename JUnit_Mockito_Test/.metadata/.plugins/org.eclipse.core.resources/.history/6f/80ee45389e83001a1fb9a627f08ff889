package com.bagic.Channelservices.domain;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

import com.bagic.Channelservices.dto.TravelDTO;
import com.bagic.Channelservices.infrastructure.TravelService;

@Component
public class TravelDomain {
	
	private int number;
	
	@Autowired
	TravelDTO tdto;
	
	@Autowired
	TravelService tsrv;

	public int getNumber() {
		tsrv.InfraTravelMethod1();
		return tdto.getNumber();
	}

	public void setNumber(int number) {
		this.tdto.setNumber(number);
	}
	
	
	
	
	
	
	

}
