package com.bagic.Channelservices.domain;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

import com.bagic.Channelservices.dto.Proposer;
import com.bagic.Channelservices.dto.TravelDTO;
import com.bagic.Channelservices.infrastructure.TravelInfastructure;

@Component
public class TravelDomain {
	
	@Autowired
	TravelDTO tdto;
	
	@Autowired
	TravelInfastructure tInfra;

	public int getNumber(Proposer p) {
		tInfra.InfraTravelMethod1(p);
		return tdto.getNumber();
	}

	public void setNumber(int number) {
		this.tdto.setNumber(number);
	}
	
	
	
	
	
	
	

}
